        "@babel/preset-typescript": "^7.24.7",
        "@faker-js/faker": "^8.4.1",
        "@linaria/babel-preset": "^5.0.4",
        "@linaria/cli": "^5.0.4",
        "@linaria/core": "^6.2.0",
        "@linaria/esbuild": "^5.0.4",
